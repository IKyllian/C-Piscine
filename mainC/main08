int main()
{
	char str1[] = "Hello";
	char str2[] = "World";
	char str3[] = "3";
	char *str[] = {str1, str2, str3 };
	t_stock_str *tab = ft_strs_to_tab(3, str);
	int i = 0;
	while (i < 4)
	{
		printf("size = %i\n", tab[i].size);
		printf("str = %s\n", tab[i].str);
		printf("copy = %s\n", tab[i].copy);
		i++;
	}
}
